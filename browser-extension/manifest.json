{
  "manifest_version": 2,
  "name": "Spaced Repetition",
  "description": "This extension allows to add some stuff to @SpaceRepBot from the browser",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "Spaced Repetition",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "activeTab",
    "contextMenus",
    "http://localhost:8080/"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}
